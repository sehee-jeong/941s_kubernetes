                                    <flatten>false</flatten>
                                    <cleanRemote>false</cleanRemote>
                                    <noDefaultExcludes>false</noDefaultExcludes>
                                    <makeEmptyDirs>false</makeEmptyDirs>
                                    <patternSeparator>[, ]+</patternSeparator>
                                    <execCommand>sh /941/secret.sh 941s sh /941/yaml_generator.sh 941s testservice941 941s.yaml 600359243171.dkr.ecr.ap-northeast-2.amazonaws.com/941s:latest 30298 80 VAR1=value1 VAR2=value2 kubectl apply -f /941/941s.yaml</execCommand>
<!--                                    <execCommand>sh /941/secret.sh {namespace}-->
<!--                                        sh /941/yaml_generator.sh {namespace} {serviceName(대문자 X)} {*.yaml} 600359243171.dkr.ecr.ap-northeast-2.amazonaws.com/{namespace}:latest 30298 80 VAR1=value1 VAR2=value2 kubectl apply -f /941/{*.yaml}</execCommand>-->
